#[start : end : step]

credit_number = "1234-5678-9012-3456"
"""
print(credit_number[3]) #4
print(credit_number[0 : 4]) # 1234. ending index is exclusive.
print(credit_number[5:9]) # 5678
print(credit_number[4:]) # -5678-9012-3456
print(credit_number[-1]) # 6
print(credit_number[5::2]) # 57-0235
"""

last_digits = credit_number[:-4]
print(last_digits) # 1234-5678-9012-
last_digits = credit_number[:4]
print(last_digits) # 1234
